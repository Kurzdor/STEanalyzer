{
    "background": {
        "scripts": [
            "index.js"
        ]
    },
    "manifest_version": 2,
    "name": "MW_steampriceanalyzer",
    "description": "Анализатор цен steam",
    "version": "1.3",
    "icons": {
      "16": "ico/favicon-16x16.png",
	  "96": "ico/favicon-96x96.png",
      "32": "ico/favicon-32x32.png"
    },	
    "permissions": [
        "http://steamcommunity.com/*",
        "http://*.steamcommunity.com/*",
        "https://steamcommunity.com/*",
        "https://*.steamcommunity.com/*",
        "storage"
    ],
    "browser_action": {
        "default_page": "index.html",
		"default_icon": "ico/favicon-32x32.png"
    }
}